{
  "name": "Todo",
  "version": "0.1",
  "description": "Todo",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/get_started16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
